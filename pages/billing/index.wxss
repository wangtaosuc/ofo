/* pages/billing/index.wxss */
.container{
  margin:0;
  padding:0;
  width:100%;
  height: 100vh;
  display:flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
}
.number,.endride{
  flex:2;
  width:100%;
  text-align: center;
}
.time{
  flex:6;
  width:100%;
  text-align: center;
  padding: 60px 0;
}
.time .time-content{
  font-size: 100rpx;
}
.endride button{
  margin-top: 40rpx;
  width:90%;
}